.ContentPane
        {
            height: 100%;
            width: 100%;
            position: relative;
            white-space: nowrap;
            min-height: inherit;
            background-color: #E0E0E0;
        }

            .ContentPane .HorizontalPane
            {
                width: 100%;
                position: relative !important;
                clear: both;
                box-sizing: border-box;               
                min-height: 50px;
            }

            .ContentPane .VerticalPane
            {
				vertical-align:top;
                position: relative !important;
                display: inline-block;
                box-sizing: border-box;                
                min-width: 50px;
                *zoom: 1;
                *display: inline;
            }

        .VerticalResize
        {
			vertical-align:top;
            width: 3px;
            background-color: #CBE6FF;
            cursor: e-resize;
            display: inline-block;
            z-index:1000;
            *zoom: 1;
            *display: inline;
			
        }

        .HorizontalResize
        {
            height: 3px;
            background-color: #CBE6FF;
            cursor: n-resize;
            display: block;
            z-index:1000;
            width: 100%;
        }

.noResize .VerticalResize,.noResize .HorizontalResize
{
	cursor:default;
}
.wrapper
{
}

	.wrapper:after
	{
		content: ".";
		display: block;
		height: 0;
		clear: both;
		visibility: hidden;
	}

	.wrapper .inlineField
	{
		float: left;
		position: relative;
	}

		.wrapper .inlineField.dockRight
		{
			float: right;
			position: relative;
		}

	.wrapper .GroupingPanel.floatDiv
	{
		float: left;
		position: relative;
	}

	.wrapper .GroupingPanel.dockRight
	{
		float: right;
		position: relative;
	}

