.control-item
{
	margin: 0;
	padding: 5px;
	cursor: move;
	font-size: 12px;
	font-weight: bold;
	color: #0f0f0f;
	font-family: Roboto;
	background: #FFF;
	border-bottom: solid 1px #e8e8e8;
	box-shadow: inset 0px 0px 0px #353B49;
	position: relative;
	white-space: nowrap;
}

	.control-item:before
	{
		font-family: fontawesome;
		font-weight: normal;
		font-size: 15px;
		width: 25px;
		display: inline-block;
		vertical-align: middle;
		content: "\f1c0";
		line-height: 19px;
		opacity: 0.6; 
	}

	.control-item.selected, .control-item:hover
	{
		transition: background 0.18s ease-in-out !important;
		-webkit-transition: background 0.18s ease-in-out !important;
		-moz-transition: background 0.18s ease-in-out !important;
		background: #fff6b5;
		box-shadow: none;
		text-shadow: 0px 1px 1px #AAA9A9 !important;
	}


	.control-item.selected
	{
	}

	.control-item.used
	{
		color: #d1c0aa;
        text-shadow:none !important;
	}

