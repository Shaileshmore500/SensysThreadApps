.PropertyList
{
	font-family: "Roboto", sans-serif;
}

	.PropertyList .prop-item
	{
		border-bottom: solid 1px #c7c7c7;
		height: 25px;
	}

		.PropertyList .prop-item select
		{
			display: initial;
			height: initial;
			border: none;
			padding: 0px;
			outline: none !important;
            margin-left: -4px;
            background-color:#fff;
		}
		.PropertyList .prop-item option
		{
			padding: 4px 3px;
			border-bottom: solid 1px #e8e8e8;
		}


		.PropertyList .prop-item input
		{
			margin: 0 !important;
			padding: 0 !important;
			border: none !important;
			height: initial !important;
			font-size: 11px;
			outline: none !important;
			box-shadow: none !important;
            margin-top: -6px !important;
		}

		.PropertyList .prop-item .ovr
		{
			position: absolute;
			opacity: 1;
			height: 12px !important;
			width: 12px !important;
			left: initial;
			right: 2px;
            pointer-events:initial;
			top: 6px;
            display: inline-block;
		}

	.PropertyList .cat
	{
		display: block;
		background-color: #e6e6e6;
		font-size: 12px;
		font-weight: bold;
		padding: 3px 5px;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
		cursor: pointer;
	}

		.PropertyList .cat:before
		{
			content: "\f147";
			font-family: Fontawesome;
			display: inline-block;
			width: 14px;
			font-weight: normal;
		}

		.PropertyList .cat.coll:before
		{
			content: "\f196";
		}

	.PropertyList .subcat
	{
		display: block;
		font-size: 12px;
		padding: 3px 5px;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
		border-bottom: solid 1px #c7c7c7;
		cursor: pointer;
	}

		.PropertyList .subcat:before
		{
			content: "\f0da";
			font-family: Fontawesome;
			display: inline-block;
			width: 12px;
		}

		.PropertyList .subcat.exp:before
		{
			content: "\f0d7";
		}

	.PropertyList .prop-item .lhs,
	.PropertyList .prop-item .rhs
	{
		display: block;
		overflow: hidden;
		height: 100%;
		text-overflow: ellipsis;
		border-right: solid 1px #c7c7c7;
		box-sizing: border-box;
		line-height: 20px;
		margin: 0;
		float: left;
		padding: 3px 3px;
		font-size: 11px;
		white-space: nowrap;
		position: relative;
	}

    .PropertyList .prop-item .lhs {
        text-indent: 6px;
        width: 45%;
        max-width: 175px;
    }

	.PropertyList .subCtr .prop-item .lhs
	{
		text-indent: 25px;
	}

	.PropertyList .prop-item .rhs
	{
        float:none;
		border: none;
	}
        .PropertyList .prop-item .rhs [type="checkbox"] {
            display:none;
        }
    .PropertyList .multiline.prop-item .lhs {
        display: block;
        border-right: none;
        float: none;
        width: initial;
        text-decoration: underline;
        cursor: pointer;
        color: #34a3e7;
    }
        .PropertyList .multiline.prop-item .lhs:after {
            content: "\f0da";
            font-family: Fontawesome;
            display: inline-block;
            margin-left: 5px;
        }
        .PropertyList .multiline.prop-item.exp .lhs:after {
            content:"\f0d7";
        }
    .PropertyList .multiline.prop-item .rhs {
        display: block;
        float: none;
        width: initial;
        border-right: solid 1px #c7c7c7;
        padding-left: 20px;
        height: 0px;
        overflow: hidden;
        transition: height 0.5s;
        padding-top:0;
        padding-bottom:0;
    }
    .PropertyList .multiline.prop-item.exp .rhs {
        display: block;
        height: 80px;
        transition: height 0.5s;
         padding-top:0;
        padding-bottom:0;
    }
    .PropertyList .multiline.prop-item,
    .PropertyList .multiline textarea {
        height: initial;
        resize: none;
    }
     
