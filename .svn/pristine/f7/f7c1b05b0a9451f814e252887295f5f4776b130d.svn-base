.ctl-hidden:before
{
	content: "";
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	border: solid 1px #c3c3c3;
	pointer-events: none;
	background: #DADADA url(/../images/hidden.png) 50% 50% repeat !important;
    opacity:0.5;
}

.ctl-disabled:before {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    border: solid 1px #c3c3c3;
    pointer-events: none;
    background: #DADADA url(/../images/disabled.png) 50% 50% repeat !important;
    opacity: 0.5;
}

.ctl-selected:before
{
	content: "";
	display: block;
	position: absolute;
	top: -1px;
	left: 0px;
	right: 0px;
	bottom: -1px;
	border: solid 2px #337ac3;
	background-color: #d7f3ff;
	pointer-events: none;
	opacity: 0.5;
    z-index:10;
}

.highlight:before
{
	content: "";
	display: block;
	position: absolute;
	top: -1px;
	left: 0px;
	right: 0px;
	bottom: -1px;
	border: dashed 0px #007b15 !important;
	opacity: 0.5;
    pointer-events:none;
}
