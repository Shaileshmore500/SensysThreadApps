.ruler
{
	margin-bottom: 0 !important;
}

	.ruler .col
	{
		position: relative;
	}

		.ruler .col:first-child:before
		{
			position: absolute;
			content: "";
			display: block;
			height: 12px;
			width: 2px;
			background-color: #bebebe;
			left: 0;
			top: -12px;
		}

		.ruler .col:after
		{
			position: absolute;
			content: "";
			display: block;
			height: 12px;
			width: 2px;
			background-color: #bebebe;
			right: 0;
			top: -12px;
		}

		.ruler .col span
		{
			position: absolute;
			right: -2px;
			font-size: 12px;
			top: -28px;
			color: #5b5b5b;
			text-shadow: 1px 1px 1px #fff;
		}

		.ruler .col._d2 span
		{
			right: -5px;
		}

.erp-SplitButton
{
