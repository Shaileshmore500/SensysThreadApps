            content: "\f142";
            font-family: fontawesome;
        }
.erp-SplitButton .btnctr {
    left: 0;
    text-align: left;
    position: absolute;
    margin: 0;
    margin-top: 35px;
    visibility: hidden;
    background: white;
    padding: 0;
    padding-bottom: 20px;
    z-index: 100;
    box-shadow: 2px 3px 7px #646464;
    min-width: 150px;
    min-height: 50px;
}
    nav .erp-SplitButton .btnctr {
