.erp-Table .colHeader TD:hover .addRow,
.erp-Table .colHeader TD > div:hover .addCol,.erp-Table .rowHeader > div:hover .addRow,
.erp-Table .colHeader TD > div:hover .delCol,.erp-Table .rowHeader > div:hover .delRow,
.erp-Table .colHeader TD > div:hover .addRow
{
	display:block;
}

.erp-Table .addCol:before,.erp-Table .addRow:before
{	
	color:#9f9f9f;
	content: "\f067";
	font-family:fontawesome;
}
.erp-Table .delCol:before,.erp-Table .delRow:before
{
	font-family:fontawesome;
	content: "\f00d";
	color:#9f9f9f;
}
.erp-Table .colHeader .addCol
{
	right: 10px;
	top: 0px;
}
.erp-Table .colHeader .delCol
{
	left: 10px;
	top: 0px;
}
.erp-Table .rowHeader .addRow
{
	bottom: 0px;
	/* right: 2px; */
}
.erp-Table .rowHeader .delRow
{top: 0px;/* left: 2px; */}

.erp-Table .table-cell
{
	min-height:48px;
    position:relative;
}
.erp-Table .cell-header.first >div
{
	width: 25px;
    z-index: 25;
}
.erp-Table .cell-header.first .addCol
{
	left: 12px;
	top: 0px;
}
.erp-Table .cell-header.first .addRow
{
	top: 11px;
}

.nav-wrapper .btnCtr.ctr {
    min-width: 60px;
    display: inline-block;
    float: right;
    border: dashed 1px #fff;
    height: 56px;
    position: relative;
}

.nav-wrapper .btnCtr.ctr >.erp-Button {
        height: 56px;
        min-width: 56px;
        line-height: 56px;
        background: none;
        font-size: 22px;
        box-shadow: none;
        border-right: solid 1px var(--primary-color-lightest);
        margin: 0;
    }
        .nav-wrapper .btnCtr.ctr >.erp-Button:first-child {
            border-left: solid 1px var(--primary-color-lightest);
        }
        .nav-wrapper .btnCtr.ctr >.erp-Button i {
            font-size: 28px;
                margin-right: 12px;
        }

.nav-wrapper .btnCtr.ctr >.erp-Button.icoBtn.round {
    padding: 0 !important;
    text-decoration: none;
    border-radius: 50%;
    margin-right: 10px !important;
    height: 48px !important;
    width: 48px !important;
    min-width: 48px !important;
    border: solid 1px var(--primary-color-lightest);
    margin-top: -4px;
}
        .nav-wrapper .btnCtr.ctr >.erp-Button.icoBtn.round  i{
                line-height: 48px;
        }
