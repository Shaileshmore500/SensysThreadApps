        right:0;
    }
.erp-SplitButton .btnctr li  .erp-Button {
   margin: 0;
   background: #fff;
    color: #898989;
    box-shadow: none;
    border: solid 1px #e7e7e7;
    border-top: none;
    border-left: none;
    border-right: none;
    display: block;
    white-space: nowrap;
    text-align: left;
    height: 48px;
    line-height: 48px;
    float:none;
}
.erp-SplitButton .erp-Button {
    margin: 0 !important;
    display: block;
    text-align: left;
}
.erp-SplitButton .btnctr li  .erp-Button span {
        float:none;/*to prevent  wrapping against icon in case of long text/*/
    }
  
    .erp-SplitButton.large .btnctr {
        margin-top: 48px;
    }
    .erp-SplitButton.small .btnctr {
        margin-top: 29px;
    }
    .erp-SplitButton:hover .btnctr {
        visibility: visible;
    }
    .erp-SplitButton.icoBtn >.fa {
        display:none;
    }
    .erp-SplitButton.icoBtn> .arrow {
