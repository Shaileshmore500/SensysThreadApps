#divLayout,#divCss,#divScript,#divServerScript{
    position: absolute;
    top: 38px;
    left: 1px;
    right: 2px;
    bottom: 4px;
}

#toolbar
{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	background-color: #f8f8f8;
}

	#toolbar a
	{
		font-family: fontawesome;
		line-height: 35px;
		width: 50px;
		display: inline-block;
		cursor: pointer;
		outline: none !important;
		font-size: 27px;
		text-align: center;
		border-right: solid 1px #e1e1e1;
		color: #494949;
		position: relative;
	}
        #toolbar a.selected {
            background: #454545;
            color: #fff;
        }
	#toolbar .save:before
	{
		content: "\f0c7";
	}

	#toolbar .saveas:before
	{
		content: "\f0c7";
		font-size: 20px;
	}

	#toolbar .saveas i:before
	{
		content: "\f0c7";
		position: absolute;
		top: 5px;
		left: 20px;
		background-color: #fff;
		height: 20px;
		display: block;
		line-height: 20px;
		font-size: 22px;
		font-style: normal;
	}

	#toolbar .undo:before
	{
		content: "\f0e2";
	}

	#toolbar .redo:before
	{
		content: "\f01e";
	}

	#toolbar .preview:before
	{
		content: "\f002";
	}
    #toolbar .designer:before
	{
		content: "\f022";
	}
	#toolbar .html:before
	{
		content: "\f1c9";
	}

	#toolbar .css:before
	{
		content: "\f016";
	}

	#toolbar .script:before
	{
		content: "\f016";
	}

	#toolbar .server:before
	{
		content: "\f016";
	}

	#toolbar .css i:before
	{
		font-family: sans-serif;
		content: "CSS";
		position: absolute;
		top: 12px;
		left: 16px;
		height: 20px;
		display: block;
		line-height: 20px;
		font-size: 8px;
		font-style: normal;
		font-weight: bold;
	}

	#toolbar .script i:before
	{
		font-family: sans-serif;
		content: "JS";
		position: absolute;
		top: 11px;
		left: 17px;
		height: 20px;
		display: block;
		line-height: 20px;
		font-size: 12px;
		font-style: normal;
		font-weight: bold;
	}

	#toolbar .server i:before
	{
		font-family: sans-serif;
		content: "C#";
		position: absolute;
		top: 11px;
		left: 17px;
		height: 20px;
		display: block;
		line-height: 20px;
		font-size: 12px;
		font-style: normal;
		font-weight: bold;
	}

#ctlDragContainer .control-item
{
	width: 100%;
