html
{
    overflow: auto;
}
html, body, form
{
    margin: 0;
    height: 100%;
}
body
{
    background: #41495b; /* Old browsers */
    background: -moz-radial-gradient(center, ellipse cover,  #41495b 0%, #2a3348 100%); /* FF3.6+ */
    background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#41495b), color-stop(100%,#2a3348)); /* Chrome,Safari4+ */
    background: -webkit-radial-gradient(center, ellipse cover,  #41495b 0%,#2a3348 100%); /* Chrome10+,Safari5.1+ */
    background: -o-radial-gradient(center, ellipse cover,  #41495b 0%,#2a3348 100%); /* Opera 12+ */
    background: -ms-radial-gradient(center, ellipse cover,  #41495b 0%,#2a3348 100%); /* IE10+ */
    background: radial-gradient(ellipse at center,  #41495b 0%,#2a3348 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#41495b', endColorstr='#2a3348',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
    height: 100%;
    width: 100%;
	background-size:cover;
	background-position:center center;
	background-repeat:no-repeat;
}
.lblPowered
{
    position: absolute;
