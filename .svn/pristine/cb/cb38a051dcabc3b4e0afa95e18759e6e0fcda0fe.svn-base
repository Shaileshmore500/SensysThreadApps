-webkit-box-shadow: 0px 0px 7px 3px #6B6B6B;
-moz-box-shadow:0px 0px 7px 3px #6B6B6B;
-o-box-shadow:0px 0px 7px 3px #6B6B6B;
}

.loginFrame .loginButton
{
	border-style: solid;
	border-width: 2px;
	border-color: #203457;
	height: 35px;
	width: 120px;
	text-decoration: none;
	display: inline-block;
	color: #fff;
	text-shadow: 0px 2px 5px #aeb0d1;
	
	font-size: 16px;
	font-family: Arial;
	font-weight: bold;
	text-align: center;
	line-height: 35px;
	-webkit-border-radius: 7px;
	-moz-border-radius: 7px;
	border-radius: 7px;
	-webkit-box-shadow: 0px 1px 16px rgba(50, 50, 50, 0.75);
	-moz-box-shadow: 0px 1px 16px rgba(50, 50, 50, 0.75);
	box-shadow: 0px 1px 16px rgba(50, 50, 50, 0.75);
	letter-spacing:3px;
}
.loginFrame .static
{
	background:#203658 url(../images/loginstatic.png) 0 0 no-repeat;
}
.loginFrame .progress
{
	background:#203658  url(../images/loginloader.gif) 0 0 no-repeat;
}

.loginFrame  .ctitle
{
	display:block;
	color:#000;
	font-size: 14px;
	font-family: Arial;
	font-weight: bold;
	text-shadow: 0px 2px 5px #808080;
	margin:8px 0 8px 8px;
}

.loginFrame .lnkCompany
{
	font-size: 14px;
	font-family: Arial;
	font-weight: bold;
	color: #009900;
	background: url(../images/AddRecord.gif) 0 0 no-repeat;
	display: inline-block;
	padding-left: 18px;
	position:absolute;
	bottom:12px;
	left:15px;
	text-shadow: 0px 2px 5px #B4ADAD;
}
.loginFrame .lnkCompany:hover
{
	color: #336600;
}
.loginFrame .lnkCfg
{
	font-size: 10px;
	font-family: Arial;
	font-weight: bold;
	color: #013489;
	padding-left: 13px;
	background: url(../images/option.gif) 0 0 no-repeat;
	line-height: 17px;
	display: inline-block;
	position: absolute;
	top: 4px;
	right: 12px;
	text-shadow: 0px 2px 5px #808080;
}

.loginFrame .lnkCfg:hover
{
	color: #0160FC;
}


.loginFrame .lnkBack
{
	font-size: 13px;
	font-family: Arial;
	font-weight: bold;
	color: #FF0000;
	padding-left: 13px;
	line-height: 17px;
	display: inline-block;
	position: absolute;
	bottom: 3px;
	right: 15px;
	text-shadow: 0px 2px 5px #808080;
	text-decoration: none;
}

.loginFrame .lnkBack:hover
{
	color: #FF0000;
}


.loginError
{
	margin:auto;
	margin-top: 20px;
	border: 1px solid #FF9F9F;
	padding: 10px;
	border-radius: 6px;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	text-align: center;
	overflow:hidden;
	background-color: #680000;
}

.loginError SPAN
{
	font-size: 13px;
	font-family: Arial;
	font-weight: bold;
	color: #FFCCCC;
}
.loginErrGreen
{
	border: 1px solid #33CC33;	
	background-color: #004100;
}
.loginErrGreen SPAN
{
	color: #D7FFD7;
}
