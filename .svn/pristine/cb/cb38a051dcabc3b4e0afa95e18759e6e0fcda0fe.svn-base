.loginBg
{
	background-position: 0 0;
	height: 100%;
	width: 100%;
	background: url(../images/loginbg.png) repeat 0 0;
	-webkit-animation: slide 180s linear infinite;
    -moz-animation: slide 180s linear infinite;
}
@-webkit-keyframes slide
{
	
	100% {background-position: -1000px -1000px;}
}
@-moz-keyframes slide
{
	
	100% {background-position: -1000px -1000px;}
}
.loginFrame
{
	height:380px;
	width:370px;	
	margin:0 auto;
	margin-top:0px;		
	text-align:center;
	overflow:hidden;
	position:relative;
}
.loginFrame .loginFrameBg
{
	opacity: 0.45;
	overflow:hidden;
	filter: alpha(opacity=20);
	
	position: absolute;
	width: 340px;
	height: 250px;
	top: 15px;
	left: 15px;


border-radius: 10px; 
-webkit-border-radius:10px; 
-moz-border-radius: 10px;
box-shadow: 0px 0px 25px -5px #E8E8E8;
-webkit-box-shadow: 0px 0px 25px -5px #E8E8E8;
-moz-box-shadow: 0px 0px 25px -5px #E8E8E8;
-o-box-shadow: 0px 0px 25px -5px #E8E8E8;

background: #8691A0; /* Old browsers */

}

#btnForgot {
 position: absolute;
    bottom: 55px;
    right: 10px;
    text-decoration: none;
    background: #343880;
    display: inline-block;
    padding: 0px 8px;
    border-radius: 10px;
    color: #fff;
}
#btnForgot:before {
    content:"\f128";
    font-family:FontAwesome;

}

.loginKey
{
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	margin: auto;
	width: 100%;	
	overflow:hidden;
	position:relative;
	
}
.loginKey .row
{
	width: 100%;	
	position:relative;
}
.loginKey .row .rowInput
{
	width: 100%;
	margin: 0px;
	padding: 10px 15px 10px 15px;
	background-color: #F4F4F4;
	border: 0px;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	font-family: Verdana;
	font-size: 18px;
	font-weight: bold;
	color: #333333;
	text-shadow: rgb(173, 170, 170) 1px 1px 3px;
	border-radius: 8px;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	-moz-box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
	-webkit-box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
	box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
}
.loginKey .row .rowLabel
{
	top:13px !important;
}
.loginCtr
{
	position:relative;
	margin: auto;
	width: 100%;
}

.loginRemember
{
	    position: absolute;
    left: 5px;
    bottom: 3px;
    font-family: sans-serif;
    font-size: 12px;
    color: #152c50;
    text-shadow: 1px 1px 1px #b5b5b5;
}
	.loginRemember *
	{
		vertical-align:middle;
		cursor:pointer;
	}
.loginEntry
{
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	
	-moz-box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
	-webkit-box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
	box-shadow: inset 2px 2px 5px rgba(0,0,0,.4);
	
	background-color: #E6EAEE;
	padding:5px;
	padding-bottom: 45px;
}
.loginEntry .row
{
	width: 100%;
	border-bottom-style: solid;	
	border-width: 1px;
	border-color: #C9CDE0;
	margin: 0px;
	position:relative;
}

.loginEntry .row .rowLabel,.loginKey .row .rowLabel
{
	position: absolute;
	top: 10px;
	left: 15px;
	font-family: Verdana;
	font-size: 13px;
	font-weight: bold;
	color: #A3A3A3;
	display:inline-block;
	width:120px;
	overflow-x:hidden;
	text-align:left;
	white-space:nowrap;
	text-shadow: rgb(255, 255, 255) 3px 3px 5px;
}
.loginEntry .row .hideLabel,.loginKey .row .hideLabel
{
   transition: all 0.20s ease-in-out !important;
    -webkit-transition: all 0.20s ease-in-out !important;
    -moz-transition: all 0.20s ease-in-out !important;
	width:0px !important;
	
}
input:-webkit-autofill {
    -webkit-box-shadow:0 0 0 50px #E6EAEE inset; /* Change the color to your own background color */
    -webkit-text-fill-color: #333;
}

input:-webkit-autofill:focus {
    -webkit-box-shadow: /*your box-shadow*/,0 0 0 50px #E6EAEE inset;
    -webkit-text-fill-color: #333;
}
.loginEntry .rowInput
{
	width: 100%;
	margin: 0px;
	padding: 10px 15px 10px 15px;
	background-color:Transparent !important;
	border: 0px;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	font-family: Verdana;
	font-size: 13px;
	font-weight: bold;
	color: #333333;
	text-shadow: rgb(173, 170, 170) 1px 1px 3px;
}

.loginEntry .firstRow .rowInput
{
	-webkit-border-top-left-radius: 10px;
-webkit-border-top-right-radius: 10px;
-moz-border-radius-topleft: 10px;
-moz-border-radius-topright: 10px;
border-top-left-radius: 10px;
border-top-right-radius: 10px;
}
.loginEntry .lastRow
{
	border-bottom:none;
}
.loginEntry .lastRow .rowInput
{
	-webkit-border-bottom-right-radius: 10px;
-webkit-border-bottom-left-radius: 10px;
-moz-border-radius-bottomright: 10px;
-moz-border-radius-bottomleft: 10px;
border-bottom-right-radius: 10px;
border-bottom-left-radius: 10px;
}

.rowInput:active, .rowInput:focus {
outline: 0;
outline-style: none;
outline-width: 0;
-moz-outline-width: 0;
}

