.page
{
	height: 600px;
	width: 600px;
}
	.page DIV
	{
		margin:0;
		padding:0;
		-webkit-touch-callout: none;
-webkit-user-select: none;
-khtml-user-select: none;
-moz-user-select: none;
-ms-user-select: none;
user-select: none;
	}
.header
{
	width: 100%;
	height: 10%;
	background-color: #F7FDFF;
	border: solid 1px #80DFFF;
}
.footer
{
	width: 100%;
	height: 10%;
	background-color: #F7FDFF;
	border: solid 1px #80DFFF;
}
.body
{
	width: 100%;
	height: 80%;
}

.wrapper
{
	padding:10px;
	border-left: solid 8px #7E7B77;

}


.left
{
	background-color: #F9FFEC;
	height: 100%;
	width: 20%;
	float: left;
	border: solid 1px #80DFFF;
	overflow:auto;
}
.right
{
	background-color: #F9FFEC;
	height:100%;
	width:17%;
	float:left;
	border: solid 1px #80DFFF;
	overflow:auto;
}
.center
{
	background-color: #FFFFC1;
	height: 100%;
	width: 60%;
	float: left;
	border: solid 1px #80DFFF;
}

.allEdge
{
	border-color: #009900;
	border-width:3px;
}
.topEdge
{
	border-top-color:#009900;
	border-top-width:3px;
}

.rightEdge
{
	border-right-color:#009900;
	border-right-width:3px;
}
.bottomEdge
{
	border-bottom-color:#009900;
	border-bottom-width:3px;
}
.leftEdge
{
	border-left-color:#009900;
	border-left-width:3px;
}



