<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="LoginCfg.aspx.cs" Inherits="SensysErp.Meta.LoginCfg" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
       <asp:Panel ID="pnlLogin" runat="server">
                <asp:Label style="color:red" ID="lblTitle" runat="server"></asp:Label><br />
                <span>Enter Config Username & Password</span><br />
                <asp:TextBox style="margin-top:5px" placeholder="username" runat="server" ID="txtUsn"></asp:TextBox><br />
                <asp:TextBox style="margin-top:5px" placeholder="password" TextMode="Password" runat="server" ID="txtPass"></asp:TextBox><br />
                 <asp:Button style="margin-top:5px" ID="btnLogin" runat="server" OnClick="btnLogin_Click" Text="Submit" />
            </asp:Panel>
    </form>
</body>
</html>
